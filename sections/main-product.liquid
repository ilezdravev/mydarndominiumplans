<section class="product-wrapper">
    <div
        class="product product--{{ section.settings.media_size }} product--{{ section.settings.gallery_layout }}  {% if product.media.size > 0 %} {% else %}{%comment%}product--no-media{%endcomment%}{% endif %}">

        <div class=" product__media-wrapper">
            <div class="main_slide">
                <div class="bages_icon">
                    <div class="reverse-button show-mobile main_reverse">
                        Reverse image
                        <svg xmlns="http://www.w3.org/2000/svg" width="15.855" height="15.93"
